<!doctype html>
<html lang="en">

<head>
    <meta charset="utf-8">
    <meta http-equiv="x-ua-compatible" content="ie=edge">

    <!-- Viewport Meta -->

    <meta name="viewport" content="width=device-width, initial-scale=1" />

    <!-- Styles -->

   <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
    <link rel="stylesheet" href="/assets/css/app.css">

    <!-- Mobile Theme Colour -->

    <meta name="theme-color" content="#13562b">

    <!-- Title and Description -->

    <title>{% block title %}{% endblock %}</title>
    <meta name="description" content="{% block description %}{% endblock %}">
    <meta name="keywords" content="Essex-Kent, Essex County, Kent, junior golf, golf, kid's golf, southland junior tour">

    <!-- Favicons -->

    <link rel="icon" href="/assets/img/fav.png">
    
    <!-- Open Graph -->

    <meta property="og:title" content="Essex Kent Jr. Golf Hughes Tour">
    <meta property="og:description" content="The Southland Junior Tour is a summer golf tournament for competitive boys and girls which has been operating since 2010.">
    <meta property="og:type" content="website">
    <meta property="og:url" content="hughes.darkroast.co">
    <meta property="og:site_name" content="Essex Kent Jr. Golf Hughes Tour">
    <meta property="og:image" content="/assets/img/og.jpg">
    

    <!--[if lt IE 9]>
     <script src="js/polyfill/html5-shiv.js"></script>
  <![endif]-->


</head>
<body>
    {% include 'partials/off-canvas.twig' %}
    <div class="{% block pageclass %}{% endblock %}" id="app">
            <div class="content">
                {% include 'partials/overlay.twig' %}
                {% block content %}{% endblock %}
            </div>
    </div>

    <script src="/assets/js/app.js"></script>

    {# You make me wanna be stupid when we talk about Plato and you ask 'green or red'... #}
</body>
</html>
